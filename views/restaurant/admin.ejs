<table class="table is-bordered is-striped is-hoverable is-fullwidth">
    <thead>
        <tr>
            <th>ID</th>
            <th>Title</th>
            <th>Restaurant</th>
            <th></th>
            <th></th>
        </tr>
    </thead>

    <% restaurant.forEach( function(restaurant) { %>
    

    <tbody>

        <tr>
            <td><%= restaurant.id %></td>
            <td><%= restaurant.title %></td>
            <td><%= restaurant.restaurant %></td>
            <td><a href="http://localhost:1337/restaurant/update/<%= restaurant.id %>">Update</a></td>
            <td><a href="http://localhost:1337/restaurant/owned/<%= restaurant.id %>">Redeemed Members</a></td>
        </tr>
    </tbody>


    <% }); %>


</table>